KB:
IF
LevelLoaded(_)
AND
DB_NightmareMode_StrongerBosses_Initialized(0)
THEN
TimerLaunch("NightmareMode_StrongerBosses_InitDelay", 0);

IF
SavegameLoaded()
AND
DB_NightmareMode_StrongerBosses_Initialized(0)
THEN
TimerLaunch("NightmareMode_StrongerBosses_InitDelay", 0);

IF
TimerFinished("NightmareMode_StrongerBosses_InitDelay")
THEN
NOT DB_NightmareMode_StrongerBosses_Initialized(0);
DB_NightmareMode_StrongerBosses_Initialized(1);


// This script initialize whenever you teleport to any way point, click a Wheel Door (one that teleports you somewhere) or load a save game.
// Loading a save game doesn't automatically triggers LevelLoaded(_) event.

// Below simply add passives and statues to all character stored in the INIT manually edited DB.

//REGION InitDev
IF
DB_NightmareMode_StrongerBosses_GlobalCharacters(_Character)
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
THEN
AddPassive(_Character,"NightmareMode_StrongerBosses");

IF
DB_NightmareMode_StrongerBosses_GlobalCharactersVect(_Character, _String)
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
THEN
DB_NOOP(1);

IF
DB_NightmareMode_StrongerBosses_LocalObjects(_Character)
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
THEN
AddPassive(_Character,"NightmareMode_StrongerBosses");

IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, _String)
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
THEN
DB_NOOP(1);

IF
DB_NightmareMode_StrongerBosses_Gods(_Character)
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
THEN
AddPassive(_Character,"NightmareMode_Gods");

IF
DB_NightmareMode_StrongerBosses_GodsVect(_Character, _String)
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
THEN
DB_NOOP(1);
//END_REGION InitDev

//REGION Death Ward on Long Rest.
IF
LongRestFinished()
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
AND
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "DeathWarded")
AND
GUIDToString((GUIDSTRING)_Character,_CharacterString)
AND
Concatenate("NightmareMode_StrongerBosses_DeathWardApply",_CharacterString,_OnlyOnceQry) // Reseting OnlyOnce QRY for each character after long resting.
AND
QRY_OnlyOnce_Reset(_OnlyOnceQry)
THEN
DB_NOOP(1);


// Applying Death Ward once per long rest.
IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "DeathWarded")
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
AND
GUIDToString((GUIDSTRING)_Character,_CharacterString)
AND
Concatenate("NightmareMode_StrongerBosses_DeathWardApply",_CharacterString,_OnlyOnceQry) // Making sure each QRY_OnlyOnce string is unique for each Character.
AND
QRY_OnlyOnce(_OnlyOnceQry)
THEN
ApplyStatus(_Character, "DEATH_WARD", -1.0, 1, NULL_00000000-0000-0000-0000-000000000000);
//END_REGION Death Ward on Long Rest.


//REGION Applying Passive for each type of foe.
IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "DeathWarded")
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
AND
HasPassive(_Character, "NightmareMode_DeathWard", 0)
THEN
AddPassive(_Character, "NightmareMode_DeathWard");


IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "StrongerBoss")
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
AND
HasPassive(_Character, "NightmareMode_CCImmunity", 0)
THEN
AddPassive(_Character, "NightmareMode_CCImmunity");

IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "LittleChief")
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
//AND
//HasPassive(_Character, "NightmareMode_LittleChief", 0)
THEN
//AddPassive(_Character, "NightmareMode_LittleChief");
DB_NOOP(1);

IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "StrongAlly")
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
//AND
//HasPassive(_Character, "NightmareMode_StrongAlly", 0)
THEN
//AddPassive(_Character, "NightmareMode_StrongAlly");
DB_NOOP(1);

IF
DB_NightmareMode_StrongerBosses_LocalObjectsVect(_Character, "TemporaryStunImmunity")
AND
DB_NightmareMode_StrongerBosses_Initialized(1)
//AND
//HasPassive(_Character, "NightmareMode_TemporaryStunImmunity", 0)
THEN
//AddPassive(_Character, "NightmareMode_TemporaryStunImmunity");
DB_NOOP(1);

//END_REGION Applying Passive for each type of foe.


// Making sure Gith Portal Master in act3 cast their spell whenever Death Ward is triggered.
//REGION Gith Portal Summoner Extra Check
IF
StatusRemoved(S_LOW_Elfsong_GithyankiShield_GateMaster_001_f9cf10cf-0901-4568-bc95-2a09cb3228f0, "DEATH_WARD", _, _)
AND
